
config wifi-iface
	option device 'radio1'
	option mode 'ap'
	option encryption 'none'
	option ssid 'vuExposed'
	option disabled '1'

config wifi-device 'radio0'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path 'platform/qca955x_wmac'
	option htmode 'HT20'
	option country 'US'

config wifi-device 'radio1'
	option type 'mac80211'
	option hwmode '11a'
	option path 'pci0000:01/0000:01:00.0'
	option htmode 'VHT80'
	option country 'US'
	option channel '44'
	option txpower '23'

config wifi-iface
	option device 'radio1'
	option network 'lan'
	option mode 'ap'
	option key 'poundballnumber2'
	option ssid 'vuExposed'
	option encryption 'none'

